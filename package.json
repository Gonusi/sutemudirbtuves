{
	"name": "eleventy-base-blog",
	"version": "9.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npm run build:all",
		"build:lt": "cross-env ELEVENTY_LANG=lt ELEVENTY_OUTPUT_DIR=_site_lt npx @11ty/eleventy",
		"build:en": "cross-env ELEVENTY_LANG=en ELEVENTY_OUTPUT_DIR=_site_en npx @11ty/eleventy",
		"build:all": "npm run build:lt && npm run build:en",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"new:tale": "node scripts/new-content.cjs tale",
		"new:diary": "node scripts/new-content.cjs diary",
		"new:map": "node scripts/new-content.cjs map",
		"images:cleanup": "node scripts/remove-originals-with-resized.cjs",
		"images:resize": "node scripts/resize-images.cjs",
		"images:watch": "node scripts/resize-images.cjs --watch",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start:lt": "cross-env ELEVENTY_LANG=lt ELEVENTY_OUTPUT_DIR=_site_lt npx @11ty/eleventy --serve --quiet",
		"start:en": "cross-env ELEVENTY_LANG=en ELEVENTY_OUTPUT_DIR=_site_en npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"chokidar": "^4.0.3",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"heic-convert": "^2.1.0",
		"luxon": "^3.5.0",
		"netlify-plugin-cache": "^1.0.3",
		"sharp": "^0.33.5",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.1"
	}
}
